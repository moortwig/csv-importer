{
    "name": "moortwig/csvimporter",
    "description": "A helper to make csv file handling less painful",
    "keywords": [
        "moortwig",
        "csv",
        "import",
        "file",
        "helpers"
    ],
    "homepage": "https://github.com/moortwig/csv-importer",
    "license": "MIT",
    "authors": [
        {
            "name": "Catarina Höstlöf",
            "email": "catarina@moortwig.se",
            "homepage": "http://moortwig.se/"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Moortwig\\CsvImporter\\": "packages/moortwig/csvimporter/src",
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moortwig\\CsvImporter\\": "packages/moortwig/csvimporter/src",
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
